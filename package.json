{
  "name": "next-routes-readme",
  "version": "0.0.13",
  "description": "Creates README badges from istanbul coverage report",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf lib/ && tsc",
    "start": "npm run build && node lib/index.js",
    "start:dev": "nodemon",
    "release": "standard-version",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix",
    "prepare": "npx husky install",
    "format": "prettier --write \"./**/*.{ts,tsx}\""
  },
  "main": "lib/index.js",
  "bin": {
    "next-routes-readme": "lib/index.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "keywords": [
    "readme",
    "markdown"
  ],
  "author": "Olavo Parno",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/olavoparno/next-routes-readme.git"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "ai": "^2.2.14",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "next": "^13.5.4",
    "next-auth": "^4.23.2",
    "nodemon": "^3.0.1",
    "openai-edge": "^1.2.2",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0"
  }
}
