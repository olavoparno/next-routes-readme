{
  "name": "next-routes-readme",
  "version": "0.0.23",
  "description": "Creates README badges from istanbul coverage report",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf lib/ && tsc",
    "start": "npm run build && node lib/index.js --dir=./examples --openapi",
    "start:dev": "nodemon",
    "release": "standard-version",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix",
    "prepare": "npx husky install",
    "format": "prettier --write \"./**/*.{ts,tsx}\""
  },
  "main": "lib/index.js",
  "bin": {
    "next-routes-readme": "lib/index.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "keywords": [
    "readme",
    "markdown"
  ],
  "author": "Olavo Parno",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/olavoparno/next-routes-readme.git"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "lcov"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "ai": "^3.4.33",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "next": "^15.0.3",
    "next-auth": "^4.24.10",
    "nodemon": "^3.1.7",
    "openai-edge": "^1.2.2",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.25.9",
    "@babel/types": "^7.26.0",
    "eslint-config-next": "^15.0.3",
    "react": "^19.0.0-rc-5c56b873-20241107",
    "react-dom": "^19.0.0-rc-5c56b873-20241107"
  }
}
